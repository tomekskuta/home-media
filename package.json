{
  "name": "home-media",
  "author": "tomekskuta",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.6.5",
  "scripts": {
    "build:css": "ts-node sass-compiler.ts",
    "watch:css": "nodemon --watch views -e scss --exec ts-node sass-compiler.ts",
    "start": "npm run build:css && node ./dist/index.js",
    "dev": "concurrently \"npm run watch:css\" \"nodemon -r tsconfig-paths/register ./index.ts\"",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "cookie-parser": "~1.4.7",
    "debug": "~4.4.0",
    "dotenv": "^16.4.7",
    "ejs": "~3.1.10",
    "express": "~5.0.1",
    "googleapis": "^146.0.0",
    "http-errors": "~2.0.0",
    "modern-normalize": "^3.0.1",
    "morgan": "~1.10.0",
    "sass": "^1.85.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.0",
    "@types/http-errors": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.27.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-import-resolver-typescript": "^4.2.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.16.2",
    "eslint-plugin-promise": "^7.2.1",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  }
}
